@article{Sanchez2018,
abstract = {Cheese-making properties (CMP) of pressed cooked cheeses (PCC) and soft cheeses (SC) 40 were predicted from mid-infrared (MIR) spectra. The traits that were best-predicted by MIR 41 spectra (as determined by comparison with reference measurements) were three measures of 42 laboratory cheese yield (CYFRESH, CYDM, and CYFAT-PROT), five coagulation traits (curd43 firming time: K10/RCT for PCC and SC, curd firmness: a for PCC and SC, and a2 for SC), 44 and one acidification trait for PCC (pH0) (0.54 {\textless} R² {\textless} 0.89). These nine traits, as well as milk 45 composition traits (fatty acid, protein, mineral, lactose, and citrate content) were then 46 predicted from 1,100,238 MIR spectra from 126,873 primiparous Montbeliarde cows. Using 47 this dataset, we estimated the corresponding genetic parameters of these traits by REML. 48 A univariate or bivariate repeatability model was used which included the fixed effects of 49 herd x test-day x spectrometer, stage of lactation, and year x month of calving, as well as the 50 random additive genetic, permanent environmental, and residual effects.},
author = {Sanchez, M-P and {El Jabri}, M and Minery, S and Wolf, V and Beuvier, E and Laithier, C and Delacroix-Buchet, A and Brochard, M and Boichard, D},
