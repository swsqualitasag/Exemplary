keywords = {MIR,fatty acid,fraction,genetic parameter,infrared spectra,protein,protein fraction},
mendeley-tags = {MIR},
number = {7},
pages = {5526--5540},
pmid = {28478002},
publisher = {American Dairy Science Association},
title = {{Genetic parameters of measures and population-wide infrared predictions of 92 traits describing the fine composition and technological properties of milk in Italian Simmental cattle}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0022030217303715},
volume = {100},
