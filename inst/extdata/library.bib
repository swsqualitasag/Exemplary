@phdthesis{Hospenthal2016,
abstract = {In Schweizer Schlachth{\"{o}}fen werden routinem{\"{a}}ssig die Fleischigkeit und die Fettgewebeklasse erhoben. In den letzten Jahren hat die durchschnittliche Fettgewebeklasse von Tieren der Markenprogramme Natura-Veal, Natura-Beef und SwissPrimBeef abgenommen. Natura- Veal sind Bankk{\"{a}}lber, die mit einem Alter von ungef{\"{a}}hr sechs Monaten direkt von der Mutter abgesetzt geschlachtet werden. Natura-Beef bezeichnet Fleisch von Jungtieren aus Mutterkuhhaltung, die als schlachtreife Absetzer mit ungef{\"{a}}hr zehn Monaten geschlachtet werden. SwissPrimBeef bezeichnet ebenfalls Fleisch aus Mutterkuhhaltung. Bis zum Absetzen wachsen die K{\"{a}}lber im Herdenverband bei der Mutter auf. Danach werden die Tiere bis zu einem Alter von ungef{\"{a}}hr 20 Monaten ausgem{\"{a}}stet. Die Entwicklung der abnehmenden Fettgewebeklasse ist nicht erwünscht. Zurzeit existiert in der Schweiz keine Zuchtwertsch{\"{a}}tzung für Fettgewebeklassen. Aus diesem Grund wurden in dieser Masterarbeit die Schlachtdaten der drei Markenprogramme von 2005 bis Mitte 2015 analysiert. In einem Modell A wurden die Fettgewebeklassen über alle Markenprogramme ausgewertet. Als fixe Effekte modelliert wurden Nettozunahme, Rasse, Geschlecht, Fleischigkeitsklasse, Markenprogramm, Markenprogramm*Alter und Markenprogramm*Alter2. Betrieb*Jahr wurde als zuf{\"{a}}lliger Effekt modelliert. In Modell B wurde jedes Markenprogramm separat ausgewertet. Modelliert wurden Nettozunahme, Rasse, Geschlecht, Fleischigkeitsklasse, Alter und Alter2 als fixe und Betrieb*Jahr als zuf{\"{a}}lliger Effekt. Dabei konnte gezeigt werden, dass sich der Effekt der Rasse auf die Fettgewebeklasse je nach Modell {\"{a}}ndert. Das heisst, es besteht eine Interaktion zwischen Markenprogramm und Rasse. W{\"{a}}hrend Angus in allen Markenprogrammen eine hohe Fettabdeckung erzielt, weisen Tiere der Rasse Grauvieh besonders im Natura-Veal eine gute Fettabdeckung auf. Als Natura-Beef weisen neben Hereford und Angus auch die Rassen Original Braunvieh, Limousin, Simmental und Aubrac gute Fettabdeckungen aus. Bei SwissPrimBeef sind es neben Hereford und Angus auch Tiere der Rasse Simmental und Salers, die eine gute Fettabdeckung aufweisen. In allen Modellen hatten weibliche Tiere eine h{\"{o}}here Fettabdeckung als m{\"{a}}nnliche kastrierte Tiere und diese hatten wiederum eine h{\"{o}}here Fettabdeckung als m{\"{a}}nnliche Tiere. Auch das Alter, das Alter2, die Nettozunahme und die Fleischigkeit haben einen signifikanten Effekt auf die Fettgewebeklasse. Die Auswertungen haben gezeigt, dass sowohl Modell A als auch Modell B akzeptable Resultate liefern. In Modell A sollte jedoch die Interaktion zwischen Markenprogramm und Rasse berücksichtigt werden. In einem n{\"{a}}chsten Schritt müssten die Modelle mit dem Tiereffekt erg{\"{a}}nzt werden, um die Verwandtschaft der Tiere zu berücksichtigen. Zus{\"{a}}tzlich sollte man genetische Gruppen für Rassen sch{\"{a}}tzen und nicht mit der Rasse mit dem h{\"{o}}chsten Blutanteil arbeiten. Eine Zuchtwertsch{\"{a}}tzung für die Fettgewebeklasse ist wichtig, um die Fleischqualit{\"{a}}t in Zukunft zu verbessern. Dabei sollte die Fettgewebeklasse und die Fleischigkeitsklasse in einem für den Landwirt anwendbarem Index vereint werden.},
author = {Hospenthal, Andrea},
file = {:Users/silvan/Library/Application Support/Mendeley Desktop/Downloaded/Hospenthal Andrea - 2016 - Pilot-study to evaluate optimal ages at slaughter in beef cattle(3).pdf:pdf},
pages = {1--89},
school = {ETH Zurich},
title = {{Pilot-study to evaluate optimal ages at slaughter in beef cattle.}},
year = {2016}
}
@article{Hardie2015,
abstract = {Abstract Prior to genomic selection on a trait, a reference population needs to be established to link marker genotypes with phenotypes. For costly and difficult-to-measure traits, international collaboration and sharing of data between disciplines may be necessary. Our aim was to characterize the combining of data from nutrition studies carried out under similar climate and management conditions to estimate genetic parameters for feed efficiency. Furthermore, we postulated that data from the experimental cohorts within these studies can be used to estimate the net energy of lactation (NEL) densities of diets, which can provide estimates of energy intakes for use in the calculation of the feed efficiency metric, residual feed intake (RFI), and potentially reduce the effect of variation in energy density of diets. Individual feed intakes and corresponding production and body measurements were obtained from 13 Midwestern nutrition experiments. Two measures of RFI were considered, RFIMcal and RFIkg, which involved the regression of NEL intake (Mcal/d) or dry matter intake (DMI; kg/d) on 3 expenditures: milk energy, energy gained or lost in body weight change, and energy for maintenance. In total, 677 records from 600 lactating cows between 50 and 275 d in milk were used. Cows were divided into 46 cohorts based on dietary or nondietary treatments as dictated by the nutrition experiments. The realized NEL densities of the diets (Mcal/kg of DMI) were estimated for each cohort by totaling the average daily energy used in the 3 expenditures for cohort members and dividing by the cohort's total average daily DMI. The NEL intake for each cow was then calculated by multiplying her DMI by her cohort's realized energy density. Mean energy density was 1.58 Mcal/kg. Heritability estimates for RFIkg, and RFIMcal in a single-trait animal model did not differ at 0.04 for both measures. Information about realized energy density could be useful in standardizing intake data from different climate conditions or management systems, as well as investigating potential genotype by diet interactions.},
author = {Hardie, L.C. and Armentano, L.E. and Shaver, R.D. and VandeHaar, M.J. and Spurlock, D.M. and Yao, C. and Bertics, S.J. and Contreras-Govea, F.E. and Weigel, K.A.},
doi = {10.3168/jds.2014-8580},
isbn = {0022-0302},
issn = {00220302},
journal = {Journal of Dairy Science},
keywords = {en-,ergy density,feed efficiency,genetic selection,residual feed intake},
mendeley-tags = {feed efficiency},
number = {4},
pages = {2727--2737},
pmid = {25660745},
title = {{Considerations when combining data from multiple nutrition experiments to estimate genetic parameters for feed efficiency}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0022030215000703},
volume = {98},
year = {2015}
}
